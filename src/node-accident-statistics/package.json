    "@types/fs-extra": "^5.0.4",
    "@types/mssql": "^4.0.12",
    "@types/shelljs": "^0.8.2",
    "fs-extra": "^7.0.1",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1"
  },
  "dependencies": {
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
